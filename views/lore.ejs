<% layout('./layouts/boilerplate') %>
<div id="roles">
    <% for (let rol in roles) { %>
        <div class="role">
            <h3 class="roleName"><%= roles[rol].name.charAt(0).toUpperCase() + roles[rol].name.slice(1) %></h3>
            <div class="line"></div>
            <li><%= roles[rol].description %></li>
           <li>Is admin: <%= roles[rol].isAdmin %></li>
            <li>Is of type: <%= roles[rol].isType %></li>
            <li>Color: <%= roles[rol].color %></li>

        </div>
        <% } %>
</div>