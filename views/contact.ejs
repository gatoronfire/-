<% layout('./layouts/boilerplate') %>

    <div id="users">
        <% for (let user in currentUsers) { %>
            <div class="user">

                <h3 id="name" class="<%= currentUsers[user].status %>">
                    <%= currentUsers[user].userNames %>
                </h3>
                <div class="line"></div>
                <div class="dropdown_content">
                    <%= currentUsers[user].description ? currentUsers[user].description
                        : 'Lorem, ipsum dolor sit amet consectetur adipisicing elit. Eius impedit aliquid doloribus laboriosam praesentium consectetur, similique sint hic assumenda, nesciunt culpa? Debitis quaerat at, fugiat eos earum aliquid sit culpa.'
                        %>

                </div>
            </div>

            <% } %>
    </div>